{
  "name": "cottage-town",
  "version": "1.0.0",
  "description": "Website for cottage-town",
  "main": "index.js",
  "scripts": {
    "build": "cuttlebelle",
    "sass-prebuild": "node-sass ./assets/sass/index.scss ./assets/css/index.css",
    "watch-sass": "node-sass -w ./assets/sass/index.scss ./assets/css/index.css",
    "watch": "yarn watch-sass &  cuttlebelle -w",
    "deploy": "gh-pages -d site"
  },
  "cuttlebelle": {
    "site": {
      "markdownRenderer": "markdown.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@splidejs/react-splide": "^0.4.4",
    "gh-pages": "^3.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "node-sass": "^5.0.0"
  }
}
